{
    "homepage": ".",
    "name": "client",
    "version": "0.0.0",
    "main": "main.js",
    "files": [
        "build",
        "*.js",
        "public"
    ],
    "scripts": {
        "electron": "ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
        "start": "ng serve --open",
        "start:electron": "ng build --base-href ./ && electron .",
        "format": "prettier --config ../.prettierrc.yaml --write \"src/**/*\"",
        "build": "ng build",
        "build:electron": "ng build --prod && electron-packager .  --asar=true --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build",
        "test": "ng test",
        "coverage": "ng test --code-coverage --sourceMap=true",
        "lint": "ng lint --type-check --format stylish",
        "lint:fix": "npm run lint -- --fix"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.9",
        "@angular/cdk": "^11.2.0",
        "@angular/common": "~10.0.9",
        "@angular/compiler": "~10.0.9",
        "@angular/core": "~10.0.9",
        "@angular/forms": "~10.0.9",
        "@angular/material": "^11.2.0",
        "@angular/platform-browser": "~10.0.9",
        "@angular/platform-browser-dynamic": "~10.0.9",
        "@angular/router": "~10.0.9",
        "bootstrap": "^4.6.0",
        "cookie-parser": "^1.4.5",
        "express": "^4.17.1",
        "font-awesome": "^4.7.0",
        "inversify": "^5.0.5",
        "morgan": "^1.10.0",
        "rxjs": "~6.5.5",
        "socket.io": "^3.1.1",
        "socket.io-client": "^3.1.1",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.5",
        "@angular/cli": "~10.0.5",
        "@angular/compiler-cli": "~10.0.9",
        "@angular/language-service": "~10.0.9",
        "@types/body-parser": "^1.19.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.9",
        "@types/express": "^4.17.11",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/morgan": "^1.9.2",
        "@types/node": "^12.11.1",
        "@types/socket.io-client": "^1.4.35",
        "codelyzer": "^6.0.0",
        "electron": "^11.2.3",
        "electron-packager": "^15.2.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-firefox-launcher": "^1.3.0",
        "karma-jasmine": "~3.3.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "^2.1.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-microsoft-contrib": "^6.2.0",
        "tslint-plugin-prettier": "^2.3.0",
        "typescript": "^3.9.7"
    }
}
